    "def solution(n, m) :\n",
    "    answer = []\n",
    "    max_ = n\n",
    "    min_ = m\n",
    "\n",
    "    if max_ < min_ :\n",
    "        max_, min_ = m, n\n",
    "        \n",
    "    while min_ > 0 :\n",
    "        max_, min_ = min_, max_%min_\n",
    "        \n",
    "    answer = [max_, (n*m)//max_]\n",
    "    \n",
    "    return answer"
   ]
